{
  "name": "oxyromon",
  "version": "0.17.0-beta",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check",
    "check:watch": "svelte-check --watch",
    "lint": "healthier && prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.23.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "bootstrap": "^5.3.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql": "^16.8.0",
    "graphql-request": "^6.1.0",
    "healthier": "^6.3.0",
    "lodash-es": "^4.17.21",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "pretty-bytes": "^6.1.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "svelte-fa": "^3.0.4",
    "svelte-preprocess": "^5.0.4",
    "sveltestrap": "^5.11.1",
    "vite": "^4.4.9"
  },
  "type": "module",
  "healthier": {
    "plugins": [
      "svelte3"
    ],
    "ignorePatterns": [
      "build/",
      "target/"
    ],
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "processor": "svelte3/svelte3"
      }
    ]
  },
  "prettier": {
    "importOrder": [
      "^@core/(.*)$",
      "^@server/(.*)$",
      "^@ui/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false
  }
}
